{
  "name": "electron-main",
  "version": "1.0.0",
  "main": "build/main.js",
  "scripts": {
    "dev:next": "cd front && next dev",
    "build:next": "cd front && next build",
    "start:next": "cd front && next start",
    "build:back": "cd back && tsc",
    "start:back": "cd back && node build/index.js",
    "watch:electron": "cross-env NODE_ENV='development' nodemon",
    "build_watch:electron": "tsup --watch",
    "dev:electron": "npm-run-all --parallel build_watch:electron watch:electron",
    "build:electron": "tsup",
    "dist:electron": "electron-builder --dir",
    "dev": "npm-run-all --parallel dev:*",
    "build": "run-s build:*",
    "dist": "run-s build dist:electron",
    "start": "run-s start:*",
    "pack:win": "npm run build && electron-builder --config electron-builder.yml",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-log": "^5.4.1",
    "express": "^5.1.0",
    "get-port-please": "^3.1.2",
    "next": "14.2.18",
    "react": "^18",
    "react-dom": "^18",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
